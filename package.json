{
  "name": "survivor-fantasy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0",
    "build": "prisma generate && next build && cp -r data .next/standalone/data && cp -r public .next/standalone/public && cp -r .next/static .next/standalone/.next/static",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "typecheck": "tsc --noEmit",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:test": "tsx prisma/seed-test.ts",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate",
    "sim:export": "Rscript src/simulation/data/export-survivor-data.R",
    "sim:import": "tsx src/simulation/data/import.ts",
    "sim:run": "tsx src/simulation/cli/simulate.ts",
    "sim:batch": "tsx src/simulation/cli/batch.ts",
    "sim:compare": "tsx src/simulation/cli/compare.ts",
    "sim:preview": "tsx src/simulation/cli/preview.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.37.3",
    "@prisma/client": "^6.19.2",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "next": "14.2.35",
    "next-pwa": "^5.6.0",
    "prisma": "^6.19.2",
    "react": "^18",
    "react-dom": "^18",
    "recharts": "^3.7.0",
    "svix": "^1.84.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@clerk/testing": "^1.13.35",
    "@playwright/test": "^1.58.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^5.1.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.35",
    "jsdom": "^28.0.0",
    "postcss": "^8",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@clerk/shared",
      "@prisma/engines",
      "prisma",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
